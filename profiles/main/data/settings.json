{
  "files.autoSave": "onFocusChange",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Default Dark+",
  "editor.codeActionsOnSave": [
    "editor.formatOnSave",
    "source.organizeImports",
    "source.sortImports",
    "source.fixAll",
    "quickfix.biome"
  ],
  "editor.formatOnPaste": true,
  "files.enableTrash": false,
  "git.enableSmartCommit": true,
  "git.autofetch": "all",
  "jest.runMode": "on-demand",
  "svg.preview.mode": "svg",
  "[dart]": {
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "pasteImage.insertPattern": "",
  "redhat.telemetry.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.openFoldersInNewWindow": "on",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
    "**/.idea": true,
    "**/.vscode": true
  },
  "search.exclude": {
    "**/.idea": true,
    "**/.vscode": true
  },
  "settingsSync.ignoredSettings": [
    "-window.zoomLevel"
  ],
  "go.toolsManagement.autoUpdate": true,
  "editor.bracketPairColorization.enabled": true,
  "typescript.suggest.paths": true,
  "javascript.suggest.paths": true,
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "go.alternateTools": {
    "go": "/snap/go/current/bin/go"
  },
  "projectManager.git.baseFolders": [
    "/home/ilteoood/Documents/git",
    "/Users/ilteoood/Documents/git",
    "/Users/Matteo_Pietro_Dazzi/Documents/git"
  ],
  "vsicons.dontShowNewVersionMessage": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "svelte.enable-ts-plugin": true,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.associations": {
    "*.mdx": "markdown"
  },
  "reveal-button.editor_active": true,
  "reveal-button.reveal_in_os_editor_active": true,
  "reveal-button.reveal_in_os_explorer_active": true,
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "editor.linkedEditing": true,
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "codeium.enableSearch": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "codeium.enableConfig": {
    "*": true,
    "dockercompose": true,
    "plaintext": true,
    "markdown": true,
    "properties": true
  },
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
"license.author": "Matteo Pietro Dazzi",
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "easy-codesnap.backgroundColor": "#abb8c3",
  "easy-codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 20px 68px",
  "easy-codesnap.containerPadding": "0",
  "easy-codesnap.roundedCorners": true,
  "easy-codesnap.showWindowControls": true,
  "easy-codesnap.showWindowTitle": false,
  "easy-codesnap.showLineNumbers": true,
  "easy-codesnap.realLineNumbers": false,
  "easy-codesnap.transparentBackground": false,
  "easy-codesnap.target": "container",
  "easy-codesnap.shutterAction": "copy",
  "dart.flutterHotReloadOnSave": "all",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "prisma.showPrismaDataPlatformNotification": false
}